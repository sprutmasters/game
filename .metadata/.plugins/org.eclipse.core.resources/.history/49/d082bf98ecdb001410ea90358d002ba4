package TileMap;

import java.awt.image.BufferedImage;

public class tile {

	private BufferedImage imagel;
	private int type;
	
	//TILETYPES
	public static final int NORMAL = 0;
	public static final int BLOCKED = 1;
	
	public Tile(BufferedImage image, int type){
		this.image = image;
		this.type = type;
	}
}
